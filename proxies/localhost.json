{
  "/api/**": {
    "target": "https://localhost:5000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": "https://localhost:5000/api"
    },
    "headers": {
      "Connection": "keep-alive"
    }
  },
  "/schemas/**": {
    "target": "https://localhost:5000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/schemas": "https://localhost:5000/schemas"
    },
    "headers": {
      "Connection": "keep-alive"
    }
  }
}
